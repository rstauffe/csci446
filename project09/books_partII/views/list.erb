<table border="1">
<tr>
<th>Rank</th>
<th>Title</th>
<th>Author</th>
<th>Language</th>
<th>Year</th>
<th>Copies</th>
</tr>

<% @books.each do |book| %>
<tr>
<td><%= book.id %></td>
<td><%= book.title %></td>
<td><%= book.author %></td>
<td><%= book.language %></td>
<td><%= book.published %></td>
<td><%= book.copies %></td>
</tr>
<% end %>
</table>